### basic plot ###

import matplotlib.pyplot as plt

pop = [3, 5, 6, 8, 9, 10, 11]
year = [1960, 1970, 1980, 1990, 2000, 2010, 2020]

plt.plot(year,pop)
plt.show()

### plot with strings ###

import pandas
from collections import Counter

a = ["C", "J", "U", "C", "C", "U"]

letter_counts = Counter(a)
df = pandas.DataFrame.from_dict(letter_counts, orient='index')
df.plot(kind='bar')
